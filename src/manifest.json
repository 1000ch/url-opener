{
  "manifest_version": 2,
  "name": "URL opener",
  "version": "0.1.5",
  "description": "Open specified urls.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": ["js/background.js"]
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/icon38.png",
      "38": "icons/icon38.png"
    }
  },
  "options_page": "html/options.html",
  "offline_enabled": true,
  "author": "1000ch"
}